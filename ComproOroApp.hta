<script	 language="VBScript">
	Sub Window_onLoad
    	arrCommands = Split(objGestSped.commandLine, chr(34))    	
    	MsgBox arrCommands
    	Rem For i = 3 to (Ubound(arrCommands) – 1) Step 2
        Rem 	MsgBox arrCommands(i)
   	 	Rem Next
	End Sub
</script>



<script>
	function DoStartup() {
		getCLParameter()
		getStoreTO()
		getStoreBM()
		getStoredSpeds()
		getStoredSpedDetts()
		displayAllSpeds()
		displayAllBM()
		displayAllTO()
		window.resizeTo(1500,765);
		initialSizeAndPos()
		if (isAdministrator()) {
			alert("sono amministratore")
		}
		
	}
	
	function isNumberKey(evt) {
  		var charCode = (evt.which) ? evt.which : evt.keyCode
  		if (charCode > 31 && (charCode < 48 || charCode > 57)) {
    		return false;
    	}
  		return true;
	}
	function isDecimalKey(evt) {
  		var charCode = (evt.which) ? evt.which : evt.keyCode
  		if (charCode != 44) {
  			return isNumberKey(evt);
    	}
  		return true;
	}

	function add_to_js() {
		alert("funzione aggiunta titolo ancora da implementare")
	}
	
	function add_bm_js() {
		add_bm()
	}
	
	function add_sped_js() {
		insert_sped();
		document.getElementById("banco_metalli").focus();
	}
	function add_det_sped_js() {
		alert("TODO add_det_sped_js")
	}
	function delete_sped_js(kt) {
		if (confirm("Confermi la cancellazione ?") == true) {
			delete_sped(kt)
		}
	}
</script>

